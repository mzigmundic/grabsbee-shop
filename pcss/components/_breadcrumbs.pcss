.breadcrumbs {
  height: pxtorem(63);
  display: flex;
  align-items: center;

  & li {
    font-size: pxtorem(12);
    display: flex;
    align-items: center;
    &:not(:last-child)::after {
      @mixin icon-font-display "\e90b";
      padding-left: pxtorem(10);
      padding-right: pxtorem(10);
    }
    &:last-child {
      font-weight: 700;
    }
  }
}