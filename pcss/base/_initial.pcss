/* Container Class Setup */
.container {
  @mixin container $container-width, $container-gutter;

  @media (--large) {
    @mixin container $container-width, $container-gutter-large;
  }
}

/* Detail Resets */
details > summary {
  outline: none;
}

details > summary::-webkit-details-marker {
  display: none;
}
